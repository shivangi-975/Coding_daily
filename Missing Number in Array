// Initial template for C++

#include <bits/stdc++.h>
using namespace std;




 // } Driver Code Ends
// User function template for C++

class Solution{
  public:
    int MissingNumber(vector<int>& array, int n) {
        // Your code goes here
        int sum=(n*(n+1))/2;
        int p=0;
        int q=0;
        for(int i=0;i<n-1;i++)
        {
            p=p+array[i];
        }
        q=sum-p;
        return q;
    }
};

